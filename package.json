{
  "name": "jscout",
  "version": "2.24.5",
  "description": "Jscout traverses the JSON structure recursively until it finds the first matching key and returns its value",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsc --declaration && rollup -c",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaniilPak/jscout.git"
  },
  "keywords": [
    "jscout",
    "json",
    "traverse",
    "utility",
    "typescript"
  ],
  "author": "Daniil P.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaniilPak/jscout/issues"
  },
  "homepage": "https://github.com/DaniilPak/jscout#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "rollup": "^4.24.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
